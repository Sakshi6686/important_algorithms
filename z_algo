 vector<int> findZ(string s){

       
        int n=s.size();
          vector<int>z(n,0);
 int i=0;int j=-1;
      
z[0]=-1;
         int l=0 ,r=0;

         for(int i=1;i<n;i++){
            if(i<r){
                z[i]=min(r-i,z[i-l]);
            }
            while(i+z[i]<n && s[z[i]]==s[i+z[i]]){
                z[i]++;
            }
            if(i+z[i]>r){
                l=i;
                r=i+z[i];
            }
         }

         return z;
    }
